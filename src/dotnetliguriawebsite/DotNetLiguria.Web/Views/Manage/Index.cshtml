@model DotNetLiguria.Web.Models.IndexViewModel

<!-- CSS Page Style -->
<link rel="stylesheet" href="~/Content/Unify/css/pages/profile.css">

<!-- CSS Implementing Plugins -->
<link rel="stylesheet" href="~/Content/Unify/plugins/animate.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/line-icons/line-icons.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/font-awesome/css/font-awesome.min.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/scrollbar/css/jquery.mCustomScrollbar.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/sky-forms-pro/skyforms/css/sky-forms.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/sky-forms-pro/skyforms/custom/custom-sky-forms.css">

<!--=== Profile ===-->
<div class="container content profile">
    <div class="row">
        <!--Left Sidebar-->
        <div class="col-md-3 md-margin-bottom-40">
            @*@<img class="img-responsive profile-img margin-bottom-20" src="@Model.ProfileImage" alt="">*@
            <ul class="list-group sidebar-nav-v1 margin-bottom-40" id="sidebar-nav-1">
                <li class="list-group-item active">
                    <a href="#"><i class="fa fa-bar-chart-o"></i> Overall</a>
                </li>
                <li class="list-group-item">
                    <a href="@Url.Action("ChangePassword","Manage")"><i class="fa fa-user"></i> Change Password</a>
                </li>
                <li class="list-group-item">
                    <a href="@Url.Action("ManageLogins","Manage")"><i class="fa fa-group"></i> Manage Logins</a>
                </li>
            </ul>
            <hr>
        </div>
        <!--End Left Sidebar-->
        <!-- Profile Content -->
        <div class="col-md-9">
            <div class="col-sm-6 md-margin-bottom-20">
                <div class="partialContents" data-url="/user/yourevents" style="display:block; margin-left:auto; margin-right:auto; min-height:200px">
                    <h3>
                        <img src="~/Content/images/loading-rectangle.gif" width="20" height="16" />
                        Loading...
                    </h3>
                </div>
            </div>
        </div>
        <!-- End Profile Content -->
    </div>
    <div class="row">
        <div class="col-md-3 md-margin-bottom-40">
        </div>
        <div class="col-md-9">
            <!--Alert-->
            <div class="col-sm-6 sm-margin-bottom-30">
                <div class="partialContents" data-url="/user/notification" style="display:block; margin-left:auto; margin-right:auto; min-height:200px">
                    <h3>
                        <img src="~/Content/images/loading-rectangle.gif" width="20" height="16" />
                        Loading...
                    </h3>
                </div>
            </div>
            <!--End Alert-->
            <!--Profile Event-->
            <div class="col-sm-6 md-margin-bottom-20">
                <div class="partialContents" data-url="/user/upcomingevents" style="display:block; margin-left:auto; margin-right:auto; min-height:200px">
                    <h3>
                        <img src="~/Content/images/loading-rectangle.gif" width="20" height="16" />
                        Loading...
                    </h3>
                </div>
            </div>
            <!--End Profile Event-->
        </div>
    </div>
</div><!--/container-->
<!--=== End Profile ===-->

<script>
    $(document).ready(function (e) {

        $(".partialContents").each(function (index, item) {
            var url = "@Url.Content("~/").TrimEnd('/')" + $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });

    });
</script>


<!-- JS Global Compulsory -->
<script type="text/javascript" src="~/Content/Unify/plugins/jquery/jquery.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/jquery/jquery-migrate.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/bootstrap/js/bootstrap.min.js"></script>

<!-- JS Implementing Plugins -->
<script type="text/javascript" src="~/Content/Unify/plugins/back-to-top.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/smoothScroll.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/counter/waypoints.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/counter/jquery.counterup.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/sky-forms-pro/skyforms/js/jquery-ui.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/scrollbar/js/jquery.mCustomScrollbar.concat.min.js"></script>
<!-- JS Customization -->
<script type="text/javascript" src="~/Content/Unify/js/custom.js"></script>
<!-- JS Page Level -->
<script type="text/javascript" src="~/Content/Unify/js/app.js"></script>
<script type="text/javascript" src="~/Content/Unify/js/plugins/datepicker.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        App.init();
        App.initCounter();
        App.initScrollBar();
        Datepicker.initDatepicker();
    });
</script>
