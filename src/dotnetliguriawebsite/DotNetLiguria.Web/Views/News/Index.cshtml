<!--=== Blog Posts ===-->
<div class="container content-md">

    <div class="partialContents" data-url="/news/popular" style="display:block; margin-left:auto; margin-right:auto;">
        <h3>
            <img src="~/Content/images/loading-rectangle.gif" width="20" height="16" />
            Loading...
        </h3>
    </div>
    <!-- End Blog All Posts -->

</div>
<!--=== End Blog Posts ===-->

<script>
    $(document).ready(function (e) {

        $(".partialContents").each(function (index, item) {
            var url = "@Url.Content("~/").TrimEnd('/')" + $(item).data("url");
            if (url && url.length > 0) {
                $(item).load(url);
            }
        });

    });</script>

<!-- JS Implementing Plugins -->
