@model IEnumerable<DotNetLiguria.Web.RssEngine.RssSchema>

@{
    ViewBag.Title = "Test";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Web Fonts -->
<link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600&amp;subset=cyrillic,latin'>

<!-- CSS Global Compulsory -->
<link rel="stylesheet" href="~/Content/Unify/plugins/bootstrap/css/bootstrap.min.css">
<link rel="stylesheet" href="~/Content/Unify/css/style.css">

<!-- CSS Header and Footer -->
<link rel="stylesheet" href="~/Content/Unify/css/headers/header-default.css">
<link rel="stylesheet" href="~/Content/Unify/css/footers/footer-v1.css">

<!-- CSS Implementing Plugins -->
<link rel="stylesheet" href="~/Content/Unify/plugins/animate.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/line-icons/line-icons.css">
<link rel="stylesheet" href="~/Content/Unify/plugins/font-awesome/css/font-awesome.min.css">

<!-- CSS Customization -->
<link rel="stylesheet" href="~/Content/Unify/css/custom.css">


@*~/Content/Unify*@

<!--=== Blog Posts ===-->
<div class="container content-md">

    @foreach (var feed in Model)
    {
        <!-- News v3 -->
        <div class="row margin-bottom-20">
            <div class="col-sm-5 sm-margin-bottom-20">
                <img class="img-responsive" src="@feed.ImageUrl" alt="">
            </div>
            <div class="col-sm-7">
                <div class="news-v3">
                    <ul class="list-inline posted-info">
                        <li>By @feed.Author</li>
                        <li>In @feed.Name</li>
                        <li>Posted @feed.PublishDate</li>
                    </ul>
                    <h2><a href="@feed.FeedUrl">@feed.Title</a></h2>
                    <p>@feed.Summary</p>
                    <ul class="post-shares">
                        <li>
                            <a href="#">
                                <i class="rounded-x icon-speech"></i>
                                <span>5</span>
                            </a>
                        </li>
                        <li><a href="#"><i class="rounded-x icon-share"></i></a></li>
                        <li><a href="#"><i class="rounded-x icon-heart"></i></a></li>
                    </ul>
                </div>
            </div>
        </div><!--/end row-->
        <!-- End News v3 -->

        <div class="clearfix margin-bottom-20"><hr></div>
    }
</div>



<!-- JS Global Compulsory -->
<script type="text/javascript" src="~/Content/Unify/plugins/jquery/jquery.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/jquery/jquery-migrate.min.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/bootstrap/js/bootstrap.min.js"></script>
<!-- JS Implementing Plugins -->
<script type="text/javascript" src="~/Content/Unify/plugins/back-to-top.js"></script>
<script type="text/javascript" src="~/Content/Unify/plugins/smoothScroll.js"></script>
<!-- JS Customization -->
<script type="text/javascript" src="~/Content/Unify/js/custom.js"></script>
<!-- JS Page Level -->
<script type="text/javascript" src="~/Content/Unify/js/app.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function () {
        App.init();
    });
</script>
